{
  "expo": {
    "scheme": "acme",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "web": {
      "output": "static"
    },
    "icon": "./assets/images/icon.png",
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://n"
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/images/splash-icon.png",
          "dark": {
            "image": "./assets/images/splash-icon.png",
            "backgroundColor": "#ffffff"
          },
          "imageWidth": 200
        }
      ]
    ],
    "name": "SPK Digital Bank",
    "slug": "spk-digitalbank-expo",
    "extra": {
      "router": {
        "origin": "https://n"
      }
    },
    "android": {
      "package": "com.dhanuprys.spkdigitalbank"
    }
  }
}
